<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Pos?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.layout.HBox?>
<Pane fx:id="rootPane" prefWidth="600" prefHeight="240" stylesheets="@../css/sales-manager.css" styleClass="root-pane" xmlns="http://javafx.com/javafx/11.0.14" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.salesController.DeleteDailyItemSalesController">
    <StackPane alignment="CENTER" prefWidth="600" prefHeight="240" styleClass="white-form" layoutX="0" layoutY="0" >
        <VBox fx:id="deletePane" alignment="CENTER" spacing="20" prefWidth="400" prefHeight="160" >
            <Label fx:id="deleteLabel" wrapText="true" styleClass="dialog-content-name" />
            <HBox alignment="CENTER" spacing="20">
                <Button fx:id="confirmDeleteButton" text="Delete" onAction="#onConfirmDeleteClicked" styleClass="btn-delete" mnemonicParsing="false" prefWidth="100" />
                <Button fx:id="cancelDeleteButton" text="Cancel" onAction="#onCancelClicked" styleClass="btn-secondary" mnemonicParsing="false" prefWidth="100" />
            </HBox>
        </VBox>

    </StackPane>
</Pane>
